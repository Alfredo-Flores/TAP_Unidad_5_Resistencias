<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="612dp"
        android:layout_height="827dp"
        android:layout_marginEnd="180dp"
        android:layout_marginRight="180dp"
        android:layout_marginBottom="50dp"
        android:elevation="-2dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/resistencia" />

    <View
        android:id="@+id/ractangle_1"
        android:layout_width="0dp"
        android:layout_height="33dp"
        android:layout_alignParentTop="true"
        android:layout_marginStart="24dp"
        android:layout_marginLeft="24dp"
        android:layout_marginTop="74dp"
        android:layout_marginEnd="31dp"
        android:layout_marginRight="31dp"
        android:layout_marginBottom="76dp"
        android:background="#DDDDDD"
        android:visibility="visible"
        app:layout_constraintBottom_toTopOf="@+id/spinner_color_3"
        app:layout_constraintEnd_toStartOf="@+id/spinner_color_1"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/textView" />

    <View
        android:id="@+id/ractangle_4"
        android:layout_width="0dp"
        android:layout_height="33dp"
        android:layout_alignParentTop="true"
        android:layout_marginStart="24dp"
        android:layout_marginLeft="24dp"
        android:layout_marginTop="87dp"
        android:layout_marginEnd="31dp"
        android:layout_marginRight="31dp"
        android:layout_marginBottom="87dp"
        android:background="#DDDDDD"
        android:visibility="visible"
        app:layout_constraintBottom_toBottomOf="@+id/resultado_ohm"
        app:layout_constraintEnd_toStartOf="@+id/spinner_color_4"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ractangle_2" />

    <View
        android:id="@+id/ractangle_2"
        android:layout_width="121dp"
        android:layout_height="28dp"
        android:layout_alignParentTop="true"
        android:layout_marginStart="38dp"
        android:layout_marginLeft="38dp"
        android:layout_marginTop="24dp"
        android:layout_marginEnd="53dp"
        android:layout_marginRight="53dp"
        android:layout_marginBottom="25dp"
        android:background="#DDDDDD"
        android:visibility="visible"
        app:layout_constraintBottom_toTopOf="@+id/ractangle_3"
        app:layout_constraintEnd_toStartOf="@+id/spinner_color_2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ractangle_1"
        app:layout_constraintVertical_bias="0.52" />

    <View
        android:id="@+id/ractangle_3"
        android:layout_width="117dp"
        android:layout_height="27dp"
        android:layout_alignParentTop="true"
        android:layout_marginStart="38dp"
        android:layout_marginLeft="38dp"
        android:layout_marginEnd="53dp"
        android:layout_marginRight="53dp"
        android:background="#DDDDDD"
        android:visibility="visible"
        app:layout_constraintBottom_toBottomOf="@+id/spinner_color_3"
        app:layout_constraintEnd_toStartOf="@+id/spinner_color_3"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/spinner_color_3" />

    <Spinner
        android:id="@+id/spinner_color_1"
        android:layout_width="0dp"
        android:layout_height="31dp"
        android:layout_marginTop="76dp"
        android:layout_marginEnd="27dp"
        android:layout_marginRight="27dp"
        android:layout_marginBottom="76dp"
        app:layout_constraintBottom_toTopOf="@+id/spinner_color_3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/ractangle_1"
        app:layout_constraintTop_toTopOf="@+id/textView" />

    <Spinner
        android:id="@+id/spinner_color_3"
        android:layout_width="0dp"
        android:layout_height="31dp"
        android:layout_marginEnd="27dp"
        android:layout_marginRight="27dp"
        android:layout_marginBottom="310dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/ractangle_3" />

    <Spinner
        android:id="@+id/spinner_color_4"
        android:layout_width="0dp"
        android:layout_height="31dp"
        android:layout_marginTop="87dp"
        android:layout_marginEnd="27dp"
        android:layout_marginRight="27dp"
        android:layout_marginBottom="87dp"
        app:layout_constraintBottom_toBottomOf="@+id/resultado_ohm"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/ractangle_4"
        app:layout_constraintTop_toBottomOf="@+id/spinner_color_2" />

    <Spinner
        android:id="@+id/spinner_color_2"
        android:layout_width="0dp"
        android:layout_height="31dp"
        android:layout_marginTop="24dp"
        android:layout_marginEnd="27dp"
        android:layout_marginRight="27dp"
        android:layout_marginBottom="23dp"
        app:layout_constraintBottom_toTopOf="@+id/ractangle_3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/ractangle_2"
        app:layout_constraintTop_toBottomOf="@+id/spinner_color_1" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="217dp"
        android:layout_height="26dp"
        android:layout_marginEnd="36dp"
        android:layout_marginRight="36dp"
        android:layout_marginBottom="176dp"
        android:text="@string/code"
        android:textSize="18sp"
        app:layout_constraintBottom_toTopOf="@+id/ractangle_3"
        app:layout_constraintEnd_toEndOf="parent" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="4dp"
        android:layout_marginRight="4dp"
        android:text="@string/ohm"
        app:layout_constraintBaseline_toBaselineOf="@+id/resultado_ohm"
        app:layout_constraintEnd_toStartOf="@+id/resultado_ohm" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="1dp"
        android:layout_marginLeft="1dp"
        android:layout_marginEnd="1dp"
        android:layout_marginRight="1dp"
        android:layout_marginBottom="105dp"
        android:text="@string/percentage"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/resultado_porcentaje"
        app:layout_constraintHorizontal_chainStyle="packed"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/resultado_ohm"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="3dp"
        android:layout_marginLeft="3dp"
        android:layout_marginBottom="32dp"
        app:layout_constraintBottom_toTopOf="@+id/resultado_porcentaje"
        app:layout_constraintStart_toStartOf="@+id/resultado_porcentaje" />

    <TextView
        android:id="@+id/resultado_porcentaje"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBaseline_toBaselineOf="@+id/textView3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView3" />

</androidx.constraintlayout.widget.ConstraintLayout>